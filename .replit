
run = "python3 main.py"
entrypoint = "main.py"
hidden = [".config", "package-lock.json", "tsconfig.json"]
modules = ["python-3.11", "nodejs-20"]

[nix]
channel = "stable-24_05"
packages = ["cargo", "libiconv", "libxcrypt", "openssl", "pkg-config", "postgresql", "rustc"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 3002
externalPort = 3002

[[ports]]
localPort = 3003
externalPort = 3003

[[ports]]
localPort = 3004
externalPort = 4200

[[ports]]
localPort = 3005
externalPort = 5000

[[ports]]
localPort = 3006
externalPort = 5173

[[ports]]
localPort = 3007
externalPort = 6000

[[ports]]
localPort = 3008
externalPort = 6800

[[ports]]
localPort = 3009
externalPort = 8000

[[ports]]
localPort = 3010
externalPort = 8008

[[ports]]
localPort = 3011
externalPort = 8080

[[ports]]
localPort = 3012
externalPort = 8081

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true

[workflows]
runButton = "Build and Start"

[[workflows.workflow]]
name = "Start Backend"
author = 45696961
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"

[[workflows.workflow]]
name = "Start Frontend"
author = 45696961
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd frontend && npm run dev"

[[workflows.workflow]]
name = "Start Full App"
author = 45696961
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd frontend && npm run dev"

[[workflows.workflow]]
name = "Build and Start"
author = 45696961
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd frontend && npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
